{
    "name": "intaro/pinba-bundle",
    "description": "Collects and sends execution time of Doctrine queries, Twig renders and Memcache requests as pinba timers to pinba server",
    "keywords": ["symfony", "pinba", "php"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [{
        "name" : "Ilyas Salikhov"
    }],
    "require": {
        "php": ">=5.5",
        "symfony/framework-bundle" : "~2.7|~3.0|~4.0|~5.0",
        "symfony/yaml": "~2.2|~3.0|~4.0|~5.0"
    },
    "suggest": {
        "ext-pinba": "Native Pinba PHP Extension",
        "vearutop/pinba-pure-php": "Pure PHP Pinba client implementation"
    },
    "autoload": {
        "psr-0": {
            "Intaro\\PinbaBundle\\": ""
        }
    },
    "target-dir" : "Intaro/PinbaBundle"
}
